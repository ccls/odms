<%= content_tag_for( :tr, follow_up_type, :class => 'row' ) do %>
 <td class='key'><%= follow_up_type.key -%></td>
 <td class='description'><%= follow_up_type.description -%></td>
 <td class='manage'>
<%= link_to( "View", follow_up_type_path(follow_up_type), :class => 'button' ) -%>&nbsp;
<%= link_to( "Edit", edit_follow_up_type_path(follow_up_type), :class => 'button' ) -%>&nbsp;
<%= form_tag follow_up_type, :method => :delete, :class => 'destroy_link_to' do %>
<%= hidden_field_tag 'confirm', "Destroy follow_up_type '#{follow_up_type}'?", :id => nil %>
<%= submit_tag 'Destroy', :name => nil %>
<% end %>
 </td>
<% end %><!-- class='follow_up_type' -->
