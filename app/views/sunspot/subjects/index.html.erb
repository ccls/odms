<div>
Insert facet selector here

</div>
<div>
<% unless @search.results.empty? %>
<table>
<thead>
<tr>
<th>id</th>
<th>subject type</th>
<th>vital status</th>
<th>case control type</th>
<th>reference_date</th>
<th>sex</th>
<th>dob</th>
<th>died_on</th>
<th>phase</th>
<th>birth_year</th>
</tr>
</thead>
<tbody>
<% @search.results.each do |result| %>
<tr>
<td><%= link_to result.id, study_subject_path(result.id) %></td>
<td><%= result.subject_type %></td>
<td><%= result.vital_status %></td>
<td><%= result.case_control_type %></td>
<td><%= result.reference_date %></td>
<td><%= result.sex %></td>
<td><%= result.dob %></td>
<td><%= result.died_on %></td>
<td><%= result.phase %></td>
<td><%= result.birth_year %></td>
</tr>
<% end %>
</tbody>
</table>
<% else %>
<p>No results</p>
<% end %>
</div>
