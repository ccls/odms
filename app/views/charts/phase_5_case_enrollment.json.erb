{
	"title":{
		"text":  "Phase 5 Case Enrollment",
		"style": "{font-size: 18px; font-family: Verdana; text-align: center;}"
	},
	"y_legend":{
		"text": "Count",
		"style": "{font-size: 12px;}"
	},
	"elements":[
		{
			"type":      "bar_glass",
			"alpha":     0.5,
			"colour":    "#CC9933",
			"font-size": 10,
			"values" :   [ <%= "#{@study_subjects.collect(&:count).sum},#{@study_subjects.select{|s|(s.is_eligible == 1) && (s.consented == 1)}.collect(&:count).sum}".html_safe %> ]
		}
	],
	"x_axis":{
		"stroke":1,
		"tick_height":10,
		"grid_colour":"#00ff00",
		"labels": {
			"labels": ["Total Cases","Total Eligible/Consented Cases", "Total Assigned Cases", "Total Interviewed Cases" ]
		}
	},
	"y_axis":{
		"stroke":      1,
		"tick_length": 3,
		"grid_colour": "#00ff00",
		"offset":      0,
		"steps":       <%=((@study_subjects.collect(&:count).sum)/4).chart_round%>,
		"max":         <%=@study_subjects.collect(&:count).sum.chart_round%>
	}
}
