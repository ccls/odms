{
	"title":{
		"text":  "Phase 5 Case Enrollment",
		"style": "{font-size: 18px; font-family: Verdana; text-align: center;}"
	},
	"y_legend":{
		"text": "Count",
		"style": "{font-size: 12px;}"
	},
	"elements":[
		{
			"type":      "bar_glass",
			"alpha":     0.5,
			"colour":    "#FF0000",
			"font-size": 10,
			"values" :   [ <%= @counts.join(',') %> ]
		}
	],
	"x_axis":{
		"stroke":1,
		"tick_height":10,
		"grid_colour":"#00ff00",
		"labels": {
			"labels": ["Total Cases","Total Eligible/Consented Cases", "Total Assigned Cases", "Total Interviewed Cases" ]
		}
	},
	"y_axis":{
		"stroke":      1,
		"tick_length": 3,
		"grid_colour": "#00ff00",
		"offset":      0,
		"steps":       <%=((@max_y)/4).chart_round%>,
		"max":         <%=@max_y.chart_round%>
	}
}
