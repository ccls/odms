{
	"title":{
		"text":  "Phase 5 Subject Type Counts",
		"style": "{font-size: 18px; font-family: Verdana; text-align: center;}"
	},
	"x_legend":{
		"text": "Subject Type",
		"style": "{font-size: 12px;}"
	},
	"y_legend":{
		"text": "Count",
		"style": "{font-size: 12px;}"
	},
	"elements":[
		{
			"type":      "bar_glass",
			"alpha":     0.5,
			"colour":    "#CC9933",
			"font-size": 10,
			"values" :   [ <%=@study_subjects.collect{|i|
"{\"top\":#{i.count},\"bottom\":null,\"on-click\":\"#{find_study_subjects_url(:phase => 5, :subject_type_id => i.subject_type_id)}\"\}" }.join(",\n").html_safe-%> ]
		}
	],
	"x_axis":{
		"stroke":1,
		"tick_height":10,
		"grid_colour":"#00ff00",
		"labels": {
			"labels": [<%=@subject_types.collect{|s|"\"#{s}\""}.join(',').html_safe%>]
		}
	},
	"y_axis":{
		"stroke":      1,
		"tick_length": 3,
		"grid_colour": "#00ff00",
		"offset":      0,
		"max":         <%=@max_y.chart_round%>
	}
}
