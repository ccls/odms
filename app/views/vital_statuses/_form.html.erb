<h4><%= ( (@vital_status.new_record?)?'Creating':'Editting' ) %> vital_status</h4>

<%= form_for(@vital_status, :html => { :class => 'edit_vital_status'} ) do |f| %>
<%= f.error_messages %>
<%= f.wrapped_text_field :key %>
<%= f.wrapped_text_field :description %>
<p>
<%= f.submit( (@vital_status.new_record?)? "Create" : "Update" )%>&nbsp;
<%= link_to 'Back', vital_statuses_path, :class => 'button' %>
</p>
<% end %>
