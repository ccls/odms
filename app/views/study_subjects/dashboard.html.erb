<%= control_bar() %>

<p>Subject Dashboard with random test graphs.</p>





<%# http://teethgrinder.co.uk/open-flash-chart-2/ %>

<% javascripts('swfobject.js') %>
<% content_for(:head) do %>
<% javascript_tag do %>
swfobject.embedSWF("/open-flash-chart.swf", "my_chart_1", "400", "300", "9.0.0", "expressInstall.swf", {"data-file":"/charts/case_control_types.json"} );
swfobject.embedSWF("/open-flash-chart.swf", "my_chart_2", "400", "300", "9.0.0", "expressInstall.swf", {"data-file":"/charts/subject_types.json"} );
swfobject.embedSWF("/open-flash-chart.swf", "my_chart_3", "400", "300", "9.0.0", "expressInstall.swf", {"data-file":"/charts/enrollments.json"} );
swfobject.embedSWF("/open-flash-chart.swf", "my_chart_4", "400", "300", "9.0.0", "expressInstall.swf", {"data-file":"/charts/vital_statuses.json"} );
<% end %>
<% end %>
<div id="my_chart_1"></div>
<div id="my_chart_2"></div>
<div id="my_chart_3"></div>
<div id="my_chart_4"></div>
