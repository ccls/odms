<%
	classes = ['row']
#	classes.push('ui-state-error') if study_subject.do_not_contact? 
%>
<%= content_tag_for( :tr, study_subject, :class => classes.join(' ') ) do %>
<td class='subjectid'>
<%= link_to study_subject.subjectid, study_subject_path(study_subject) -%>
</td>
<td class='studyid'>
<%= study_subject.studyid_to_s %>
</td>
<td class='full_name'>
<%= study_subject.full_name %>
</td>
<td class='reference_date'>
<%=mdy study_subject.reference_date %>
</td>
<!--
<td class='icons'><span class="ui-icon ui-icon-alert"></span></td>
-->
<% end %><%# content_tag_for %>
