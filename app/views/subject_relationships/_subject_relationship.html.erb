<%= content_tag_for( :tr, subject_relationship, :class => 'row' ) do %>
 <td class='key'><%= subject_relationship.key -%></td>
 <td class='description'><%= subject_relationship.description -%></td>
 <td class='manage'>
<%= link_to( "View", subject_relationship_path(subject_relationship), :class => 'button' ) -%>&nbsp;
<%= link_to( "Edit", edit_subject_relationship_path(subject_relationship), :class => 'button' ) -%>&nbsp;
<%= form_tag subject_relationship, :method => :delete, :class => 'destroy_link_to' do %>
<%= hidden_field_tag 'confirm', "Destroy subject_relationship '#{subject_relationship}'?", :id => nil %>
<%= submit_tag 'Destroy', :name => nil %>
<% end %>
 </td>
<% end %><!-- class='subject_relationship' -->
