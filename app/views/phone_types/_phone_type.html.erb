<%= content_tag_for( :tr, phone_type, :class => 'row' ) do %>
 <td class='key'><%= phone_type.key -%></td>
 <td class='description'><%= phone_type.description -%></td>
 <td class='manage'>
<%= link_to( "View", phone_type_path(phone_type), :class => 'button' ) -%>&nbsp;
<%= link_to( "Edit", edit_phone_type_path(phone_type), :class => 'button' ) -%>&nbsp;
<%= form_tag phone_type, :method => :delete, :class => 'destroy_link_to' do %>
<%= hidden_field_tag 'confirm', "Destroy phone_type '#{phone_type}'?", :id => nil %>
<%= submit_tag 'Destroy', :name => nil %>
<% end %>
 </td>
<% end %><!-- class='phone_type' -->
