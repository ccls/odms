<%= content_tag_for( :tr, instument_version, :class => 'row' ) do %>
 <td class='key'><%= instument_version.key -%></td>
 <td class='description'><%= instument_version.description -%></td>
 <td class='manage'>
<%= link_to( "View", instument_version_path(instument_version), :class => 'button' ) -%>&nbsp;
<%= link_to( "Edit", edit_instument_version_path(instument_version), :class => 'button' ) -%>&nbsp;
<%= form_tag instument_version, :method => :delete, :class => 'destroy_link_to' do %>
<%= hidden_field_tag 'confirm', "Destroy instument_version '#{instument_version}'?", :id => nil %>
<%= submit_tag 'Destroy', :name => nil %>
<% end %>
 </td>
<% end %><!-- class='instument_version' -->
