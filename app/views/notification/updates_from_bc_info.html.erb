<html>
<body>
<% if @bc_infos.length > 0 %>
<p>The following subjects may have changed during <%=@bc_info_file%> processing ...<p>
<table>
<thead><tr><th>ICF Master ID</th><th>Events</th><th>Changed?</th></thead>
<tbody>
<% @bc_infos.each do |bc_info| %>
<tr>
<td><%= link_to bc_info.study_subject.icf_master_id, study_subject_url(bc_info.study_subject) %></td>
<td><%= link_to 'Events', study_subject_events_url(bc_info.study_subject) %></td>
<td><%= bc_info.changes.present? %></td>
</tr>
<% end %>
</tbody></table>

<% else %>
<p>No changes generated by <%=@bc_info_file%>.<p>
<% end %>
</body>
</html>
