<html>
<body>
<% if @study_subjects.length > 0 %>
<p>The following subjects may have changed during <%=@bc_info_file%> processing ...<p>
<table><tbody>
<thead><tr><th>ICF Master ID</th><th>Events</th><th>Changed?</th></thead>
<% @study_subjects.each do |study_subject| %>
<tr>
<td><%= link_to study_subject.icf_master_id, study_subject_url(study_subject) %></td>
<td><%= link_to 'Events', study_subject_events_url(study_subject) %></td>
<td><%= study_subject.instance_variable_get("@bc_info_changed") %></td>
</tr>
<% end %>
</tbody></table>

<% else %>
<p>No changes generated by <%=@bc_info_file%>.<p>
<% end %>
</body>
</html>
