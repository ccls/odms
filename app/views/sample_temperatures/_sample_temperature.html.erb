<%= content_tag_for( :tr, sample_temperature, :class => 'row' ) do %>
 <td class='key'><%= sample_temperature.key -%></td>
 <td class='description'><%= sample_temperature.description -%></td>
 <td class='manage'>
<%= link_to( "View", sample_temperature_path(sample_temperature), :class => 'button' ) -%>&nbsp;
<%= link_to( "Edit", edit_sample_temperature_path(sample_temperature), :class => 'button' ) -%>&nbsp;
<%= form_tag sample_temperature, :method => :delete, :class => 'destroy_link_to' do %>
<%= hidden_field_tag 'confirm', "Destroy sample_temperature '#{sample_temperature}'?", :id => nil %>
<%= submit_tag 'Destroy', :name => nil %>
<% end %>
 </td>
<% end %><!-- class='sample_temperature' -->
