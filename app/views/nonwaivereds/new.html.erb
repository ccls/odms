<%# stylesheets('base/jquery.ui.all.css') %><%# for datepicker %>
<% stylesheets(:raf) %>

<h3>Rapid Ascertainment Form (RAF) - non-waiver version</h3>

<% form_for(@study_subject, :url => nonwaivered_path,
	:html => { :class => 'nonwaivered raf edit_study_subject' }) do |f| %>

<%= f.error_messages %>

<h3>Section 1: Preliminary Information
<span><span class='required'>*</span>required fields</span>
</h3>
<%= render :partial => 'raf/prelim_form', :locals => {:f=>f} %>
<%= render :partial => 'study_subjects/child_parent_form', :locals => {:f=>f} %>
<%= render :partial => 'raf/current_address_form', :locals => {:f=>f} %>
<h3>Section 2: Confirm Eligibility</h3>
<%= render :partial => 'eligibility_form', :locals => {:f=>f} %>
<h3>Section 3: Document Informed Consent</h3>
<%= render :partial => 'consent_form', :locals => {:f=>f} %>
<h3>Section 4: Biological Samples</h3>
<%= render :partial => 'samples_form', :locals => {:f=>f} %>

<p class='submit_bar'><%= f.submit_link_to 'Submit' %></p>
<% end %>
