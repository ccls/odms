<%# sub_menu_for(@study_subject) %>

<%= form_for(@abstract,:url => abstract_tdt_path(@abstract)) do |f| %>

<%= f.error_messages %>

<fieldset><legend><%= @page_title %></legend>

<%= f.wrapped_yndk_select(:tdt_report_found,
	:label_text => "Found?") %>
<%= f.wrapped_date_text_field(:tdt_report_on,
	:label_text => "Date" ) %>
<%= f.wrapped_check_box(:tdt_found_in_flow_cyto_chart,
	:label_text => "Check if found in Flow Cytometry (part C1)" ) %>
<%= f.wrapped_check_box(:tdt_found_in_separate_report,
	:label_text => "Check if done separately from Flow Cytometry and report results below" ) %>
<%= f.wrapped_pos_neg_select(:tdt_positive_or_negative,
	:label_text => "Pos/Neg" ) %>
<%= f.wrapped_text_field(:tdt_numerical_result,
	:label_text => "Num result" ) %>

</fieldset>

<%#= f.submit_bar(controller.class.name) %>
<%= f.submit_bar() %>
<% end %>
