<% stylesheets(:live_birth_data_update) %>

<p>Path: <%= @live_birth_data_update.csv_file.path %></p>
<p>File: <%= @live_birth_data_update.csv_file_file_name %></p>
<p>Size: <%= @live_birth_data_update.csv_file_file_size %></p>
<p>Updated: <%= @live_birth_data_update.csv_file_updated_at %></p>

<% unless @live_birth_data_update.csv_file_file_name.nil? %>
<pre><%= IO.read(@live_birth_data_update.csv_file.path) %></pre>
<% end %>

<%= button_to( 'Parse', parse_live_birth_data_update_path(@live_birth_data_update) )%>
<%= link_to( 'Edit', edit_live_birth_data_update_path(@live_birth_data_update), :class => 'button' ) %>
<%= link_to( 'Back', live_birth_data_updates_path, :class => 'button' ) %>
