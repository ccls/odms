<% require 'fastercsv' -%>
<%# 
        adding :headers => true is supposed to set a flag in the file 
        somewhere marking it as having a header row, but I don't 
        think that it actually does.
-%>
<%= %w( studyid icf_master_id name sex bornincali state_id_no dob ).to_csv(
  :headers => true) -%>
<% @bc_requests.each do |bc_request| -%>
<% study_subject = bc_request.study_subject -%>
<%=[
 study_subject.identifier.studyid,
 study_subject.icf_master_id_or_notice,
 study_subject.full_name,
 study_subject.sex,
 'bornincali?',
 study_subject.state_id_no,
 mdy(study_subject.dob)
].to_csv -%>
<% end -%><%# @bc_requests.each do |bc_request| -%>
