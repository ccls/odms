<%= content_tag_for(:tr,event, :class => 'row') do %>
 <td><%=mdy event.occurred_on %></td>
 <td><%= event.enrollment.project.key %></td>
 <td><%= event.operational_event_type.event_category %></td>
 <td><%= event.to_s %></td>
 <td><%= ( current_user.may_edit_events? ) ? link_to( 'edit', edit_event_path(event) ) : '&nbsp;'%></td>
<% end %>
