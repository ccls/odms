<tr>
<%# This form shouldn't actually be reloaded so preseting 
		a radio button seems like it would be unnecessary %>
<td><%= radio_button_tag('duplicate_id', duplicate.id ) %></td>
<td><%= label_tag("duplicate_id_#{duplicate.id}",duplicate.full_name) %></td>
<td><%= duplicate.studyid %></td>
<td><%= duplicate.childid %></td>
<%#
	Not all duplicates will be cases, so won't have a patient or organization
%>
<td><%= duplicate.organization.try(:code) %></td>
<td><%=mdy duplicate.dob %></td>
<td><%= duplicate.sex %></td>
</tr>
