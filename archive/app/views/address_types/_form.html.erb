<h4><%= ( (@address_type.new_record?)?'Creating':'Editting' ) %> address_type</h4>

<%= form_for(@address_type, :html => { :class => 'edit_address_type'} ) do |f| %>
<%= f.error_messages %>
<%= f.wrapped_text_field :key %>
<%= f.wrapped_text_field :description %>
<p>
<%= f.submit( (@address_type.new_record?)? "Create" : "Update" )%>&nbsp;
<%= link_to 'Back', address_types_path, :class => 'button' %>
</p>
<% end %>
